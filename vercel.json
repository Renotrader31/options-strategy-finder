{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "framework": "nextjs",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "POLYGON_API_KEY": "@polygon_api_key",
    "NODE_ENV": "production"
  }
}